<!DOCTYPE html>
<html>
  <head>
    <title><%= full_title(yield(:title)) %></title>
    <%= render partial: "layouts/rails_default" %>
    <%= render partial: "layouts/shim" %>
  </head>
  <body>
    <%= render partial: "layouts/header" %>
    <div class="container">
      <%= yield %>
